# Location of your dotfiles
# DOTFILES=.dotfiles

# dotfiles to symlink
#
# srcfile:dstfile
# dstfile will be '.$srcfile' by default
#
[files]
##bin:bin
dotsyncrc
##fasdrc
##gitconfig
##gitignore
##gnupg
##grc
##hgrc
##inputrc
##screenrc
##ssh
##subversion
##task
##taskrc
##tmux.conf
##tmuxinator
##pythonrc
##vim
##vimrc
##zlogin
##zlogout
##zprofile
##zsh
##zshenv
##zshrc
[endfiles]

# Hosts to sync
## hostname                     type=masterhost, ANY or NONE
[hosts]
##your-machine-name-here          git=NONE
[endhosts]
